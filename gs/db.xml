<module name="db">
	
	<module name="auth">
		<bean name="Account">
			<field id="2" name="createtime" type="long"/>
			<field id="3" name="roleids" type="set:long"/>
		</bean>
	
		<table name="Accounts" key="string" value="Account"/>
	</module>
	
	
	<module name="login">	
		<bean name="Role">
			<field id="2" name="userid" type="long"/>
			<field id="3" name="createtime" type="long"/>
			<field id="1" name="name" type="string"/>
			<field id="4" name="gender" type="int"/>
			<field id="5" name="profession" type="int"/>
		</bean>
		<table name="Roles" key="long" value="Role"/>
		<table name="Rolenames2id" key="string" value="long"/>
	</module>


</module>